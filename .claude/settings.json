{
    "editor.autoClosingQuotes": "always",
    "editor.fontLigatures": false,
    "diffEditor.maxFileSize": 1000,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "explorer.confirmDelete": false,
    "json.schemaDownload.enable": true,
    "php.validate.executablePath": "",
    "editor.dropIntoEditor.preferences": [],
    "terminal.integrated.automationProfile.linux": {},
    "git.autofetch": true,
    "claudeCode.preferredLocation": "panel",
    "workbench.editor.autoLockGroups": {
        "antigravity.globalArtifactsEditorInput": true
    },
    "workbench.editor.defaultBinaryEditor": "antigravity.globalArtifactsEditorInput",
    "containers.contexts.description": [],
    "antigravity.marketplaceExtensionGalleryServiceURL": "https://marketplace.visualstudio.com/_apis/public/gallery",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingComments": "always",
    "editor.codeActionsOnSave": {},
    "claudeCode.allowDangerouslySkipPermissions": true,
    "remote.antigravitySSH.experimental.disableServerChecksum": true,
    "claudeCode.useTerminal": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "json.schemas": [],
    "claudeCode.claudeProcessWrapper": "https://aistudio.google.com/apps",
    "claudeCode.environmentVariables": [],
    "workbench.settings.applyToAllProfiles": [
        "claudeCode.disableLoginPrompt"
    ],
    "claudeCode.initialPermissionMode": "bypassPermissions",
    "containers.commands.build": "${containerCommand} build --pull --rm -f \"${dockerfile}\" -t ${tag} \"${context}\"",
    "javascript.referencesCodeLens.enabled": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "antigravity.marketplaceGalleryItemURL": "https://marketplace.visualstudio.com/items",
    "editor.wordWrap": "on",
    "editor.cursorStyle": "underline-thin",
    "terminal.integrated.windowsEnableConpty": false,
    "typescript.tsserver.watchOptions": "vscode",
    "typescript.preferences.organizeImports": {},
    "terminal.integrated.automationProfile.windows": {}
}